{
  "name": "fun2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint src",
    "coverage": "jest --coverage",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "force-clean": "npm run android:clean && rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean && npm i",
    "android:clean": "cd android/ && ./gradlew clean",
    "android:build": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew clean && ./gradlew assembleRelease && ./gradlew installRelease",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "lint-fix": "eslint src --fix",
    "lint-diff": "git diff --name-only --cached --relative | grep \\.js$ | xargs eslint",
    "precommit": "npm run lint-diff"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "apisauce": "^1.1.1",
    "i18next": "^19.0.3",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-native": "0.62.3",
    "react-native-config": "0.11.7",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-localize": "^1.3.2",
    "react-native-reanimated": "^1.7.0",
    "react-native-screens": "^2.0.0-alpha.25",
    "react-native-splash-screen": "^3.2.0",
    "react-navigation": "^4.0.10",
    "react-navigation-animated-switch": "^0.4.1",
    "react-navigation-redux-helpers": "^4.0.1",
    "react-navigation-stack": "^2.0.15",
    "react-navigation-tabs": "^2.7.0",
    "react-redux": "^7.1.3",
    "reactotron-apisauce": "^3.0.0",
    "reactotron-react-native": "^4.0.3",
    "reactotron-redux": "^3.1.2",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-seamless-immutable": "^2.0.0",
    "redux-recompose": "^2.2.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-import-glob": "^2.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-wolox": "^3.0.2",
    "eslint-config-wolox-react": "^2.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "jest-react-native": "^18.0.0",
    "jsdom": "^16.0.0",
    "jsdom-global": "^3.0.2",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "react-native-mock-render": "^0.1.8",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>__tests__/setup/setupEnzyme.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/setup/",
      "<rootDir>/__tests__/redux/store.js",
      "<rootDir>/__tests__/redux/utils.js",
      "<rootDir>/__tests__/responses/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/@react-native-community/async-storage/(?!(lib))"
    ]
  }
}
